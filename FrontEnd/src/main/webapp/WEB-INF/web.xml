<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">


    <servlet>
        <servlet-name>MainModule ClientSearchService</servlet-name>
        <servlet-class>server.file.search.ClientFileServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainModule ClientSearchService</servlet-name>
        <url-pattern>/MainModule/ClientSearchService</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MainModule NewOrderhService</servlet-name>
        <servlet-class>server.order.NewOrderServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainModule NewOrderhService</servlet-name>
        <url-pattern>/MainModule/NewOrderService</url-pattern>
    </servlet-mapping>
</web-app>
